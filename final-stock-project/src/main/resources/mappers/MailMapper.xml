<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kh.st.boot.dao.MailDAO">

	<insert id="setMailCode">
	insert into email_verification(evc_mb_id, evc_code)
	value(#{ec_email}, #{code})
	</insert>

	<select id="getMailCode" resultType="MailVO">
	select * from email_verification where evc_mb_id = #{ec_email}
	</select>
</mapper>